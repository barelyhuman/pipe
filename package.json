{
  "name": "@barelyhuman/pipe",
  "version": "0.2.0-beta.5",
  "repository": "barelyhuman/pipe",
  "license": "MIT",
  "author": "reaper <ahoy@barelyhuman.dev>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "scripts": {
    "build": "node build.js",
    "types": "tsc",
    "dev": "node build.js -w",
    "test": "uvu -r tsm test",
    "fix": "prettier --write .",
    "next": "bumpp"
  },
  "prettier": "@barelyhuman/prettier-config",
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "bumpp": "^8.2.1",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "tiny-glob": "^0.2.9",
    "tsm": "^2.3.0",
    "typescript": ">=3.0.0",
    "uvu": "^0.5.6",
    "watcher": "^2.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
